    <EngineIncludes>$(SolutionDir)Includes\Engine105FE\</EngineIncludes>
    <EngineLibraries>$(SolutionDir)Includes\Engine105FE\</EngineLibraries>
  </PropertyGroup>
  <PropertyGroup />
